{
  "manifest_version": 3,
  "name": "Focus Up",
  "description": "Help stay focused by limiting use of youtube",
  "version": "0.0.0.1",
  "content_scripts": [
    {
      "js": ["jquery.min.js", "./main.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ],
  "action": {
    "default_icon": "images/brain2.png"
  },
  "icons": {
    "16": "images/brain16.png",
    "32": "images/brain32.png",
    "48": "images/brain48.png",
    "128": "images/brain128.png"
  }
}
